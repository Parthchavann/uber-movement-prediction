# Environment Variables Template
# Copy this file to .env and update the values

# Application Environment
NODE_ENV=production
ENVIRONMENT=production

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
REACT_APP_API_URL=http://localhost:8000

# Database Configuration (if using real database)
# DATABASE_URL=postgresql://user:password@localhost:5432/uberflow
# REDIS_URL=redis://localhost:6379

# Model Configuration
MODEL_PATH=/app/models
DATA_PATH=/app/data

# Security
# JWT_SECRET=your-jwt-secret-key
# API_KEY=your-api-key

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Monitoring
# SENTRY_DSN=your-sentry-dsn
# PROMETHEUS_ENABLED=true

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60

# CORS Configuration
CORS_ORIGINS=http://localhost:3000,http://localhost:3001,http://localhost:80

# Feature Flags
ENABLE_CACHING=true
ENABLE_METRICS=true
ENABLE_REAL_TIME=trueREACT_APP_API_URL=http://localhost:8000
ENVIRONMENT=local
