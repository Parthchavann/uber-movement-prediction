# Traffic AI Dashboard Environment Configuration

# Development server port
PORT=3001

# API Configuration
REACT_APP_API_URL=http://localhost:8000
REACT_APP_WS_URL=ws://localhost:8000

# Map Configuration
REACT_APP_MAP_CENTER_LAT=37.7749
REACT_APP_MAP_CENTER_LON=-122.4194

# Feature Flags
REACT_APP_ENABLE_REAL_TIME=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_ANALYTICS=true

# Theme Configuration
REACT_APP_DEFAULT_THEME=dark
REACT_APP_ENABLE_THEME_TOGGLE=true

# Performance Configuration
REACT_APP_CHART_UPDATE_INTERVAL=3000
REACT_APP_MAP_UPDATE_INTERVAL=5000

# Development Configuration
GENERATE_SOURCEMAP=true
FAST_REFRESH=true